<?xml version="1.0"?>
<config>
    <modules>
        <Test_Test>
            <version>0.2.0</version>
        </Test_Test>
    </modules>
    <global>
        <models>
            <test_test>
                <class>Test_Test_Model</class>
                <resourceModel>test_test_resource</resourceModel>
            </test_test>
            <test_test_resource>
                <class>Test_Test_Model_Resource</class>
            </test_test_resource>
        </models>
        <blocks>
            <test_test>
                <class>Test_Test_Block</class>
            </test_test>
        </blocks>
        <helpers>
            <test_test>
                <class>Test_Test_Helper</class>
            </test_test>
        </helpers>
        <resources>
            <test_test_setup>
                <setup>
                    <module>Test_Test</module>
                </setup>
            </test_test_setup>
        </resources>
        <events>
            <controller_front_init_routers>
                <observers>
                    <test_test_model_observer>
                        <type>singleton</type>
                        <class>Test_Test_Model_Observer</class>
                        <method>checkForConfigRequest</method>
                    </test_test_model_observer>
                </observers>
            </controller_front_init_routers>
        </events>
    </global>
    <frontend>
        <routers>
            <test_test>
                <use>standard</use><!--admin-->
                <args>
                    <module>Test_Test</module>
                    <frontName>test_test</frontName>
                </args>
            </test_test>
        </routers>
        <layout>
            <updates>
                <test_test>
                    <file>test.xml</file>
                </test_test>
            </updates>
        </layout>
    </frontend>
</config>
